@model IEnumerable<UserManagement.Domain.ResultModel<MemberBulkImportVM>>
@using  UserManagement.Domain.ViewModel
@if (Model != null && Model.Count() > 0)
{
    <table class="table">
        <thead>
            <tr>
                <th>
                    Errors
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Model.HFName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Model.HFEmail)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <ul>
                            @foreach (var message in item.Messages)
                            {
                                <li>@message</li>
                            }
                        </ul>
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Model.HFName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Model.HFEmail)
                    </td>
                </tr>
            }
        </tbody>
    </table>
}










































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































